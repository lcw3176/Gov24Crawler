<Window x:Class="Gov24Crawler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gov24Crawler"
        mc:Ignorable="d"
        Title="AutoGov24" Height="450" Width="800">
    <Grid>
        <ContentControl BorderThickness="0"
                        Content="{Binding SelectedViewModel, UpdateSourceTrigger=PropertyChanged}"
                        Background="Transparent"/>
    </Grid>
</Window>
